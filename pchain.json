    "doNotRules": {
      "gradients": [
        "DO NOT apply hero background gradients to cards or buttons",
        "DO NOT use card gradients on text elements",
        "DO NOT apply button gradients to icon containers"
      ],
      "colors": [
        "DO NOT use navigation text colors for body text",
        "DO NOT apply button background colors to card backgrounds",
        "DO NOT use icon fill colors for text content"
      ],
      "shadows": [
        "DO NOT apply card shadows to text elements",
        "DO NOT use button shadows on navigation elements",
        "DO NOT apply hero shadows to form inputs"
      ],
      "spacing": [
        "DO NOT use button padding for card padding",
        "DO NOT apply hero spacing to component spacing",
        "DO NOT use icon container padding for text padding"
      ],
      "typography": [
        "DO NOT use heading font weights for body text",
        "DO NOT apply hero font sizes to card content",
        "DO NOT use caption colors for primary text"
      ]
    },
    "implementationNotes": {
      "colorUsage": "Always specify exact hex values and opacity levels for layered elements",
      "gradientApplication": "Apply gradients only to specified background elements, never to text or icons",
      "shadowContext": "Shadows should enhance depth hierarchy - cards lift above background, buttons lift above cards",
      "responsiveDesign": "Scale typography and spacing proportionally across breakpoints",
      "accessibility": "Maintain minimum 4.5:1 contrast ratio for text elements"
